        <div class="card">
          <div class="card-header">
            
          <h2>
            <i style='font-size:28px' class='fas'></i> Prepaid</h2>

            </div>
          <% @products.each do |product| %>
            <div class="card-body">
              <h2>
              <i style='font-size:34px' class='far'>&#xf1c5;</i> <a class="card-title"><%= product.brand%></a></h2>
              
              <br>
              <hr style="margin-top:10px; margin-bottom: 5px; width: 90%; border-color: #D0D0D0"/>
              
            </div>
          <%end%>

        </div>
      </div>
      
    
    </div>


</body>
